        <xsd:import namespace="http://www.w3.org/ns/corevocabulary/location" schemaLocation="CoreLocation-v1.00.xsd"/>
        <!--  By importing the original W3C Location vocabulary you could now "extend" it with new elements... -->
        <xsd:element name="Location" type="cl:CvlocationType"/>
        
